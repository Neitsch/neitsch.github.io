{
    "private": true,
    "scripts": {
        "build": "next build",
        "commit": "cz",
        "dev": "concurrently \"next dev\" \"nodemon --exec 'next lint --fix' -e ts,tsx,js,jsx\" \"pnpm run graphql-codegen -- --watch\" \"pnpm run test -- --coverage=false --watch\"",
        "export": "next export",
        "graphql-codegen": "graphql-codegen --config codegen.yml",
        "lint": "next lint",
        "mutation-test": "stryker run",
        "prepare": "husky install",
        "serve:prod": "pnpm run build:prod && cd out && python3 -m http.server",
        "sitemap": "next-sitemap",
        "size-build": "pnpm run graphql-codegen && pnpm run build && pnpm run sitemap && pnpm run export",
        "size": "size-limit",
        "start": "next start",
        "test": "jest",
        "ts:watch": "pnpm run ts -- --watch",
        "ts": "tsc --noEmit --incremental"
    },
    "lint-staged": {
        "{components,pages,lib}/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "pnpm run test -- --bail --findRelatedTests --coverage=false"
        ]
    },
    "dependencies": {
        "@apollo/client": "3.6.9",
        "@emotion/cache": "11.10.3",
        "@emotion/react": "11.10.0",
        "@emotion/server": "11.10.0",
        "@emotion/styled": "11.10.0",
        "@mui/lab": "5.0.0-alpha.96",
        "@mui/material": "5.10.2",
        "@sentry/nextjs": "7.11.1",
        "classnames": "2.3.1",
        "concurrently": "7.3.0",
        "date-fns": "2.29.2",
        "graphql": "16.6.0",
        "highlight.js": "11.6.0",
        "jest-environment-jsdom": "28.1.3",
        "next": "12.2.5",
        "next-images": "1.8.4",
        "next-offline": "5.0.5",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-markdown": "8.0.3",
        "react-share": "4.4.0",
        "rehype-highlight": "5.0.2",
        "sharp": "0.30.7"
    },
    "devDependencies": {
        "@babel/core": "7.18.13",
        "@babel/register": "7.18.9",
        "@commitlint/cli": "17.0.3",
        "@commitlint/config-conventional": "17.0.3",
        "@commitlint/cz-commitlint": "17.0.3",
        "@graphql-codegen/cli": "2.11.8",
        "@graphql-codegen/fragment-matcher": "3.3.1",
        "@graphql-codegen/introspection": "2.2.1",
        "@graphql-codegen/typescript": "2.7.3",
        "@graphql-codegen/typescript-document-nodes": "2.3.3",
        "@graphql-codegen/typescript-operations": "2.5.3",
        "@graphql-codegen/typescript-react-apollo": "3.3.3",
        "@size-limit/file": "8.0.1",
        "@stryker-mutator/core": "6.1.2",
        "@stryker-mutator/jest-runner": "6.1.2",
        "@swc-node/jest": "1.5.2",
        "@testing-library/jest-dom": "5.16.5",
        "@testing-library/react": "13.3.0",
        "@trivago/prettier-plugin-sort-imports": "3.3.0",
        "@types/react": "18.0.17",
        "@typescript-eslint/eslint-plugin": "5.35.1",
        "@typescript-eslint/parser": "5.35.1",
        "autoprefixer": "10.4.8",
        "babel-cli": "6.26.0",
        "babel-jest": "28.1.3",
        "commitizen": "4.2.5",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.22.0",
        "eslint-config-next": "12.2.5",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-graphql": "4.0.0",
        "eslint-plugin-jest": "26.8.7",
        "eslint-plugin-jsx-a11y": "6.6.1",
        "eslint-plugin-only-error": "1.0.2",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-react": "7.29.4",
        "eslint-plugin-react-hooks": "4.6.0",
        "graphql-tag": "2.12.6",
        "husky": "8.0.1",
        "identity-obj-proxy": "3.0.0",
        "jest": "28.1.3",
        "lint-staged": "13.0.3",
        "next-sitemap": "3.1.21",
        "next-transpile-modules": "9.0.0",
        "nodemon": "2.0.19",
        "postcss": "8.4.16",
        "prettier": "2.7.1",
        "prettier-eslint": "15.0.1",
        "react-test-renderer": "18.2.0",
        "size-limit": "8.0.1",
        "typescript": "4.8.2"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "size-limit": [
        {
            "path": "out/_next/static/chunks/**/*.js",
            "name": "static-files"
        }
    ]
}
