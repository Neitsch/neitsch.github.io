{
    "private": true,
    "scripts": {
        "dev": "concurrently \"next dev\" \"nodemon --exec 'next lint --fix' -e ts,tsx,js,jsx\" \"npm run graphql-codegen -- --watch\"",
        "build": "next build",
        "start": "next start",
        "export": "next export",
        "serve:prod": "npm run build:prod && cd out && python3 -m http.server",
        "ts": "tsc --noEmit --incremental",
        "ts:watch": "npm run ts -- --watch",
        "graphql-codegen": "graphql-codegen --config codegen.yml",
        "lint": "next lint",
        "sitemap": "next-sitemap",
        "test": "jest",
        "mutation-test": "stryker run",
        "prepare": "husky install",
        "commit": "cz",
        "size": "size-limit",
        "size-build": "npm run graphql-codegen && npm run build && npm run sitemap && npm run export"
    },
    "lint-staged": {
        "{components,pages,lib}/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "npm run test -- --bail --findRelatedTests --coverage=false"
        ]
    },
    "dependencies": {
        "@apollo/client": "^3.4.12",
        "@emotion/cache": "latest",
        "@emotion/react": "latest",
        "@emotion/server": "latest",
        "@emotion/styled": "latest",
        "@material-ui/core": "next",
        "@sentry/nextjs": "^6.13.0",
        "classnames": "2.3.1",
        "concurrently": "^6.2.1",
        "date-fns": "2.24.0",
        "graphql": "^15.5.3",
        "highlight.js": "^11.2.0",
        "next": "latest",
        "next-images": "^1.8.1",
        "next-offline": "^5.0.5",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-markdown": "^7.0.1",
        "react-share": "^4.4.0",
        "rehype-highlight": "^5.0.0",
        "sharp": "^0.29.1"
    },
    "devDependencies": {
        "@babel/core": "^7.15.5",
        "@babel/register": "^7.15.3",
        "@commitlint/cli": "^13.1.0",
        "@commitlint/config-conventional": "^13.1.0",
        "@commitlint/cz-commitlint": "^13.1.0",
        "@graphql-codegen/cli": "2.2.0",
        "@graphql-codegen/fragment-matcher": "3.1.0",
        "@graphql-codegen/introspection": "2.1.0",
        "@graphql-codegen/typescript": "2.2.2",
        "@graphql-codegen/typescript-document-nodes": "2.1.4",
        "@graphql-codegen/typescript-operations": "2.1.2",
        "@graphql-codegen/typescript-react-apollo": "3.1.2",
        "@size-limit/file": "^5.0.4",
        "@stryker-mutator/core": "^5.4.0",
        "@stryker-mutator/jest-runner": "^5.4.0",
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^12.1.0",
        "@trivago/prettier-plugin-sort-imports": "^2.0.4",
        "@types/react": "^17.0.22",
        "@typescript-eslint/eslint-plugin": "^4.31.1",
        "@typescript-eslint/parser": "^4.31.1",
        "autoprefixer": "10.3.4",
        "babel-cli": "^6.26.0",
        "babel-jest": "^27.2.1",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.32.0",
        "eslint-config-next": "11.1.2",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-graphql": "^4.0.0",
        "eslint-plugin-jest": "^24.4.2",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-only-error": "^1.0.2",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.25.3",
        "eslint-plugin-react-hooks": "^4.2.0",
        "graphql-tag": "^2.12.5",
        "husky": "^7.0.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^27.2.0",
        "lint-staged": "^11.1.2",
        "next-sitemap": "^1.6.168",
        "nodemon": "^2.0.12",
        "postcss": "8.3.6",
        "prettier": "^2.4.1",
        "prettier-eslint": "^13.0.0",
        "react-test-renderer": "^17.0.2",
        "size-limit": "^5.0.3",
        "typescript": "^4.4.3"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "size-limit": [
        {
            "path": "out/_next/static/chunks/**/*.js"
        }
    ]
}
