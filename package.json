{
    "private": true,
    "scripts": {
        "build": "next build",
        "commit": "cz",
        "dev": "concurrently \"next dev\" \"nodemon --exec 'next lint --fix' -e ts,tsx,js,jsx\" \"pnpm run graphql-codegen -- --watch\" \"pnpm run test -- --coverage=false --watch\"",
        "export": "next export",
        "graphql-codegen": "graphql-codegen --config codegen.yml",
        "lint": "next lint",
        "mutation-test": "stryker run",
        "prepare": "husky install",
        "serve:prod": "pnpm run build:prod && cd out && python3 -m http.server",
        "sitemap": "next-sitemap",
        "size-build": "pnpm run graphql-codegen && pnpm run build && pnpm run sitemap && pnpm run export",
        "size": "size-limit",
        "start": "next start",
        "test": "jest",
        "ts:watch": "pnpm run ts -- --watch",
        "ts": "tsc --noEmit --incremental"
    },
    "lint-staged": {
        "{components,pages,lib}/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "pnpm run test -- --bail --findRelatedTests --coverage=false"
        ]
    },
    "dependencies": {
        "@apollo/client": "3.5.6",
        "@emotion/cache": "11.7.1",
        "@emotion/react": "11.7.1",
        "@emotion/server": "11.4.0",
        "@emotion/styled": "11.6.0",
        "@mui/lab": "5.0.0-alpha.63",
        "@mui/material": "5.2.7",
        "@sentry/nextjs": "6.16.1",
        "classnames": "2.3.1",
        "concurrently": "7.0.0",
        "date-fns": "2.28.0",
        "graphql": "16.2.0",
        "highlight.js": "11.3.1",
        "next": "12.0.7",
        "next-images": "1.8.4",
        "next-offline": "5.0.5",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-markdown": "7.1.2",
        "react-share": "4.4.0",
        "rehype-highlight": "5.0.2",
        "sharp": "0.29.3"
    },
    "devDependencies": {
        "@babel/core": "7.16.7",
        "@babel/register": "7.16.7",
        "@commitlint/cli": "16.0.1",
        "@commitlint/config-conventional": "16.0.0",
        "@commitlint/cz-commitlint": "16.0.0",
        "@graphql-codegen/cli": "2.3.1",
        "@graphql-codegen/fragment-matcher": "3.2.1",
        "@graphql-codegen/introspection": "2.1.1",
        "@graphql-codegen/typescript": "2.4.2",
        "@graphql-codegen/typescript-document-nodes": "2.2.2",
        "@graphql-codegen/typescript-operations": "2.2.2",
        "@graphql-codegen/typescript-react-apollo": "3.2.3",
        "@size-limit/file": "7.0.5",
        "@stryker-mutator/core": "5.5.1",
        "@stryker-mutator/jest-runner": "5.5.1",
        "@swc-node/jest": "1.4.3",
        "@testing-library/jest-dom": "5.16.1",
        "@testing-library/react": "12.1.2",
        "@trivago/prettier-plugin-sort-imports": "3.1.1",
        "@types/react": "17.0.38",
        "@typescript-eslint/eslint-plugin": "5.9.0",
        "@typescript-eslint/parser": "5.9.0",
        "autoprefixer": "10.4.1",
        "babel-cli": "6.26.0",
        "babel-jest": "27.4.6",
        "commitizen": "4.2.4",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "7.32.0",
        "eslint-config-next": "12.0.7",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-graphql": "4.0.0",
        "eslint-plugin-jest": "25.3.4",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-only-error": "1.0.2",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.28.0",
        "eslint-plugin-react-hooks": "4.3.0",
        "graphql-tag": "2.12.6",
        "husky": "7.0.4",
        "identity-obj-proxy": "3.0.0",
        "jest": "27.4.7",
        "lint-staged": "12.1.5",
        "next-sitemap": "1.6.203",
        "next-transpile-modules": "9.0.0",
        "nodemon": "2.0.15",
        "postcss": "8.4.5",
        "prettier": "2.5.1",
        "prettier-eslint": "13.0.0",
        "react-test-renderer": "17.0.2",
        "size-limit": "7.0.5",
        "typescript": "4.5.4"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "size-limit": [
        {
            "path": "out/_next/static/chunks/**/*.js",
            "name": "static-files"
        }
    ]
}
