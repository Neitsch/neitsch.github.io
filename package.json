    "gatsby-plugin-nprogress": "^2.0.0-rc.1",
    "gatsby-plugin-offline": "^2.0.0-rc.9",
    "gatsby-plugin-react-helmet": "^3.0.0-rc.1",
    "gatsby-plugin-sass": "^2.0.0-rc.2",
    "gatsby-plugin-sharp": "^2.0.0-rc.7",
    "gatsby-plugin-sitemap": "^2.0.0-rc.2",
