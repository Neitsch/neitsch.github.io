{
    "private": true,
    "scripts": {
        "build": "next build",
        "commit": "cz",
        "dev": "concurrently \"next dev\" \"nodemon --exec 'next lint --fix' -e ts,tsx,js,jsx\" \"pnpm run graphql-codegen -- --watch\" \"pnpm run test -- --coverage=false --watch\"",
        "export": "next export",
        "graphql-codegen": "graphql-codegen --config codegen.yml",
        "lint": "next lint",
        "mutation-test": "stryker run",
        "prepare": "husky install",
        "serve:prod": "pnpm run build:prod && cd out && python3 -m http.server",
        "sitemap": "next-sitemap",
        "size-build": "pnpm run graphql-codegen && pnpm run build && pnpm run sitemap && pnpm run export",
        "size": "size-limit",
        "start": "next start",
        "test": "jest",
        "ts:watch": "pnpm run ts -- --watch",
        "ts": "tsc --noEmit --incremental"
    },
    "lint-staged": {
        "{components,pages,lib}/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "pnpm run test -- --bail --findRelatedTests --coverage=false"
        ]
    },
    "dependencies": {
        "@apollo/client": "3.6.4",
        "@emotion/cache": "11.7.1",
        "@emotion/react": "11.9.0",
        "@emotion/server": "11.4.0",
        "@emotion/styled": "11.8.1",
        "@mui/lab": "5.0.0-alpha.82",
        "@mui/material": "5.8.0",
        "@sentry/nextjs": "6.19.7",
        "classnames": "2.3.1",
        "concurrently": "7.2.0",
        "date-fns": "2.28.0",
        "graphql": "16.5.0",
        "highlight.js": "11.5.1",
        "jest-environment-jsdom": "28.1.0",
        "next": "12.1.6",
        "next-images": "1.8.4",
        "next-offline": "5.0.5",
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "react-markdown": "8.0.3",
        "react-share": "4.4.0",
        "rehype-highlight": "5.0.2",
        "sharp": "0.30.4"
    },
    "devDependencies": {
        "@babel/core": "7.17.12",
        "@babel/register": "7.17.7",
        "@commitlint/cli": "17.0.0",
        "@commitlint/config-conventional": "17.0.0",
        "@commitlint/cz-commitlint": "17.0.0",
        "@graphql-codegen/cli": "2.6.2",
        "@graphql-codegen/fragment-matcher": "3.2.1",
        "@graphql-codegen/introspection": "2.1.1",
        "@graphql-codegen/typescript": "2.4.11",
        "@graphql-codegen/typescript-document-nodes": "2.2.11",
        "@graphql-codegen/typescript-operations": "2.4.0",
        "@graphql-codegen/typescript-react-apollo": "3.2.14",
        "@size-limit/file": "7.0.8",
        "@stryker-mutator/core": "6.0.2",
        "@stryker-mutator/jest-runner": "6.0.2",
        "@swc-node/jest": "1.5.2",
        "@testing-library/jest-dom": "5.16.4",
        "@testing-library/react": "13.2.0",
        "@trivago/prettier-plugin-sort-imports": "3.2.0",
        "@types/react": "18.0.9",
        "@typescript-eslint/eslint-plugin": "5.25.0",
        "@typescript-eslint/parser": "5.25.0",
        "autoprefixer": "10.4.7",
        "babel-cli": "6.26.0",
        "babel-jest": "28.1.0",
        "commitizen": "4.2.4",
        "cz-conventional-changelog": "3.3.0",
        "eslint": "8.15.0",
        "eslint-config-next": "12.1.6",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-graphql": "4.0.0",
        "eslint-plugin-jest": "26.2.2",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-only-error": "1.0.2",
        "eslint-plugin-prettier": "4.0.0",
        "eslint-plugin-react": "7.29.4",
        "eslint-plugin-react-hooks": "4.5.0",
        "graphql-tag": "2.12.6",
        "husky": "8.0.1",
        "identity-obj-proxy": "3.0.0",
        "jest": "28.1.0",
        "lint-staged": "12.4.1",
        "next-sitemap": "2.5.20",
        "next-transpile-modules": "9.0.0",
        "nodemon": "2.0.16",
        "postcss": "8.4.14",
        "prettier": "2.6.2",
        "prettier-eslint": "15.0.0",
        "react-test-renderer": "18.1.0",
        "size-limit": "7.0.8",
        "typescript": "4.6.4"
    },
    "config": {
        "commitizen": {
            "path": "@commitlint/cz-commitlint"
        }
    },
    "size-limit": [
        {
            "path": "out/_next/static/chunks/**/*.js",
            "name": "static-files"
        }
    ]
}
