    "gatsby-remark-responsive-iframe": "^2.0.0-rc.5",
    "gatsby-source-filesystem": "^2.0.1-rc.6",
    "gatsby-transformer-remark": "^2.1.1-rc.5",
    "gatsby-transformer-sharp": "^2.1.1-rc.3",
    "lodash": "4.17.5",
    "lodash-webpack-plugin": "^0.11.4",
    "node-sass": "^4.9.0",
    "prismjs": "^1.15.0",
    "react": "^16.2.0",
    "react-disqus-comments": "^1.1.1",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-instantsearch": "^4.5.1",
    "react-md": "^1.2.11",
    "react-share": "^2.0.0",
    "react-typography": "^0.16.13",
    "typography": "^0.16.17"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gh-pages": "^1.1.0",
    "prettier": "^1.10.2",
    "remark-cli": "^5.0.0",
    "remark-preset-lint-recommended": "^3.0.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^9.0.0",
    "stylelint-config-standard": "^18.1.0",
    "write-good": "^0.11.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "deploy": "gatsby build && gh-pages -b master -d public",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/sample-posts/",
    "write-good": "write-good $(glob 'content/sample-posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
